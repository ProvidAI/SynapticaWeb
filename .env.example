# OpenAI API
OPENAI_API_KEY=your_openai_api_key_here

# Hedera Testnet Configuration
HEDERA_NETWORK=testnet
HEDERA_ACCOUNT_ID=0.0.12345
HEDERA_PRIVATE_KEY=your_hedera_private_key_here
HEDERA_EVM_RPC_URL=https://testnet.hashio.io/api

#Pinata
PINATA_API_KEY=pinata_key
PINATA_SECRET_KEY=pinata_secret_key

# Task Escrow Contract
TASK_ESCROW_ADDRESS=0xYourDeployedEscrowAddressHere
TASK_ESCROW_MARKETPLACE_TREASURY=0xYourMarketplaceTreasuryAddressHere
TASK_ESCROW_DEFAULT_APPROVALS=2
TASK_ESCROW_MARKETPLACE_FEE_BPS=500
TASK_ESCROW_VERIFIER_FEE_BPS=200
TASK_ESCROW_OPERATOR_PRIVATE_KEY=0xYourEscrowOperatorPrivateKeyHere
TASK_ESCROW_DEFAULT_VERIFIERS=0xVerifierOne,0xVerifierTwo

# Database
# For SQLite (recommended for development/demo):
DATABASE_URL=sqlite:///./hedera_marketplace.db
# For PostgreSQL (production):
# DATABASE_URL=postgresql://user:password@localhost:5432/hedera_marketplace

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_SECRET_KEY=your_secret_key_here_generate_with_openssl_rand_hex_32

# ERC-8004 Registry Contract
ERC8004_REGISTRY_ADDRESS=0x1234567890abcdef1234567890abcdef12345678
ERC8004_RPC_URL=https://testnet.hashio.io/api

# Agent Configuration (OpenAI Models)
ORCHESTRATOR_MODEL=gpt-4-turbo-preview
NEGOTIATOR_MODEL=gpt-4-turbo-preview
EXECUTOR_MODEL=gpt-4-turbo-preview
VERIFIER_MODEL=gpt-4-turbo-preview
# Alternative models: gpt-4, gpt-3.5-turbo

# A2A Agent Endpoints (optional, fall back to local execution when unset)
NEGOTIATOR_A2A_URL=http://127.0.0.1:9101
EXECUTOR_A2A_URL=http://127.0.0.1:9102
VERIFIER_A2A_URL=http://127.0.0.1:9103
A2A_EVENT_WEBHOOK_URL=

# Override host/port when running standalone agent servers
NEGOTIATOR_A2A_HOST=0.0.0.0
NEGOTIATOR_A2A_PORT=9101
EXECUTOR_A2A_HOST=0.0.0.0
EXECUTOR_A2A_PORT=9102
VERIFIER_A2A_HOST=0.0.0.0
VERIFIER_A2A_PORT=9103

# Logging
LOG_LEVEL=INFO
